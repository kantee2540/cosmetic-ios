   pod 'Crashlytics', '~> 3.14.0'
   pod 'Firebase/Analytics'
   pod 'Firebase/Auth'
   pod 'AFNetworking', '~> 4.0'

  target 'CosmeticTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CosmeticUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
